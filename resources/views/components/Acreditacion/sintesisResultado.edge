<div class="row">
    <div class="col-md-12">
        <div class="portlet">
            <div class="portlet-heading bg-primary">
                <h5 class="portlet-title">{{titulo}}</h5>
                <div class="clearfix"></div>
            </div>
            <div id="bg-default" class="panel-collapse collapse in show">
                <div class="portlet-body">
                    <div class="row">
                    <div class="col-md-2">
                    
                    </div>
                    <div class="col-md-8">
                    @each(item in data)
                    <div class="row">
                        <div class="col-md-4 text-center"><h3><i class="dripicons-document-new"></i></h3></div>
                        <div class="col-md-4 text-center"><h5>{{item.Nombre}}</h5></div>
                        <div class="col-md-4 text-center">
                            <h5>
                                @if(item.Estado == "REPROBADO")
                                    <span class="label label-danger">{{item.Estado}}</span>
                                @else
                                    <span class="label label-success">{{item.Estado}}</span>
                                @endif
                                
                            </h5>
                        </div>
                    </div>
                    @endeach
                    
