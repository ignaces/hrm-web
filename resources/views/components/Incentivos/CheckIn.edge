<script>
    function hazCheckin()
    {
        if($("#poscheckin").val() == "0")
        {
            swal("Check-in", "Debes seleccionar un punto de venta para realizar el check-in.", "error");
        }
        else
        {
            swal("Check-in", "Check-in realizado correctamente.", "success");
            $("#panel-checkin").modal("close");
        }
        
    }
</script>