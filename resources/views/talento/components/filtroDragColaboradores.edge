                                                    <div class="form-group">
                                                                    <label class="control-label">TR</label>
                                                            
                                                                        <select id="cmbCuadrante" class="select2 form-control select2-multiple buscar" multiple="multiple" multiple data-placeholder="Choose ...">
                                                                            <option value="-1" selected>Todos</option>
                                                                            @each(cuadrante in cuadrantes)
                                                                            <option value="{{cuadrante.idCuadrante}}">{{cuadrante.nombreCuadrante}}</option>
                                                                            @endeach
                                                                        </select>
                                                            
                                                            
                                                    </div>
                                                </div>

                                                <div class="col-sm-3">
                                                    <div class="form-group">
                                                                    <label class="control-label">Jefatura Directa</label>
                                                            
                                                                        <select id="cmbJefatura" class="select2 form-control select2-multiple buscar" multiple="multiple" multiple data-placeholder="Choose ...">
                                                                            <option value="-1" selected>Todos</option>
                                                                            @each(jefe in jefatura)
                                                                            <option value="{{jefe.id}}">{{jefe.nombres}} {{jefe.apellidoPaterno}} {{jefe.apellidoMaterno}}</option>
