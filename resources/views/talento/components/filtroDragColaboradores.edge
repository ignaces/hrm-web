                                                <div class="col-sm-3">
                                                    <div class="form-group">
                                                                    <label class="control-label">Cargos</label>
                                                            
                                                                        <select id="cmbCargo" class="select2 form-control select2-multiple buscar" multiple="multiple" multiple data-placeholder="Selecciona uno más cargos...">
                                                                            <option value="-1" selected>Todos</option>
                                                                            @each(cargos in cargosPorEmpresa)
                                                                            <option value="{{cargos.idCargo}}">{{cargos.nombreCargos}}</option>
                                                                            @endeach
                                                                        </select>
                                                            
                                                            
                                                    </div>
                                                </div>
