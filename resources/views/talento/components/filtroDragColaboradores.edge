                                                                        <div class="form-group">
                                                                            <label class="control-label">{{clasificaciones.nombrePadre}}</label>
                                                                            <div class="">
                                                                    
                                                                                <select id="buscar" class="select2 form-control select2-multiple buscarCla" multiple="multiple" multiple data-placeholder="Selecciona una o más {{clasificaciones.nombrePadre}}">
                                                                                    <option value="-1" selected>Todos</option>
